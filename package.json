{
  "name": "check-files-word",
  "version": "0.0.1-beta.1",
  "packageManager": "pnpm@7.1.7",
  "description": "Check that the words in the project are spelled correctly",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cfw": "./bin/cli.js"
  },
  "keywords": [
    "check file word"
  ],
  "scripts": {
    "dev": "cd playground && pnpm run dev",
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "fix": "eslint --fix --ext .js,.ts src tests",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist/",
    "types/",
    "bin"
  ],
  "author": "cyly 786156072@qq.com",
  "homepage": "https://github.com/ChpShy/check-files-word#readme",
  "bugs": {
    "url": "https://github.com/ChpShy/check-files-word/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChpShy/check-files-word.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitest/ui": "^0.7.4",
    "bumpp": "^8.2.1",
    "c8": "^7.11.0",
    "changelogithub": "^0.12.4",
    "esbuild": "^0.14.27",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.16.3",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "typescript": "^4.1",
    "vite": "^2.8.6",
    "vitest": "^0.7.4"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "dictionary-en-gb": "^2.4.0",
    "fast-glob": "^3.2.11",
    "picocolors": "^1.0.0",
    "retext": "^8.1.0",
    "retext-spell": "^5.1.0",
    "tsup": "^5.12.6",
    "vfile-reporter": "^7.0.4"
  }
}
